    "moment": "^2.29.2",
    "next": "12.0.8",
    "next-seo": "^5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
